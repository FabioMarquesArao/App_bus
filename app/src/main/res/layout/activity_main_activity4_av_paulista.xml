<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"


    tools:context=".MainActivity4_AvPaulista">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar4"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:background="@drawable/faixa_airport"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Escolha uma opção!"
        android:textAllCaps="true"
        android:textColor="@color/cinza"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar4" />


    <Button
        android:id="@+id/button13"
        android:layout_width="243dp"

        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/cinzaTransp"
        android:text="Boa Viagem"
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:rippleColor="#C5CAE9" />

    <Button
        android:id="@+id/button14"
        android:layout_width="243dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/cinzaTransp"
        android:text="Tietê"
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button13"
        app:rippleColor="#C5CAE9" />

    <Button
        android:id="@+id/button15"
        android:layout_width="243dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/cinzaTransp"
        android:text="estação luz"
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button14"
        app:rippleColor="#C5CAE9" />

    <Button
        android:id="@+id/button16"
        android:layout_width="243dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/cinzaTransp"
        android:text="República"
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button15"
        app:rippleColor="#C5CAE9" />

    <Button
        android:id="@+id/button17"
        android:layout_width="243dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"

        android:backgroundTint="@color/cinzaTransp"
        android:text="ibis pta"
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button16"
        app:rippleColor="#C5CAE9" />

    <Button
        android:id="@+id/button18"
        android:layout_width="243dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/cinzaTransp"
        android:text="h.renaissance"
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button17"
        app:rippleColor="#C5CAE9" />

    <Button
        android:id="@+id/button19"
        android:layout_width="243dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/cinzaTransp"
        android:text="h.intercont."
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button18"
        app:rippleColor="#C5CAE9" />

    <Button
        android:id="@+id/button20"
        android:layout_width="243dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"


        android:backgroundTint="@color/cinzaTransp"
        android:text="h.macksoud"
        android:textColor="@color/cinza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button19"
        app:rippleColor="#C5CAE9" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="08dp"
        android:text="By FabioMarquesArao"
        android:textColor="@color/cinza"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="21dp" />

</androidx.constraintlayout.widget.ConstraintLayout>